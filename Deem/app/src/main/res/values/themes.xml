<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Deem" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/purple_500</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <!-- Status bar color.00ffffff -->
        <item name="android:statusBarColor" tools:targetApi="l">#000000</item>

    </style>

    <style name="Theme.Deem.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.CorPoints.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.CorPoints.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="Button.White" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">@android:color/white</item>
    </style>

    <style name="Button.Orange" parent="ThemeOverlay.AppCompat">
        <item name="colorAccent">#fd8469</item>
    </style>

    <style name="RoundedImage20">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">20dp</item>
    </style>
    <style name="RoundedImage100">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">100dp</item>
    </style>
</resources>